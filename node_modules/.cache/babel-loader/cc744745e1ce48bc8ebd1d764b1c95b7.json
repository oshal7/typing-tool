omponent_base_dialog","title","onClose","default","actions","_createVNode","_component_base_button","onClick"],"sourceRoot":"","sources":["/Users/oshalurade/Downloads/project-01/src/components/lesson/Keyboard.vue"],"sourcesContent":["<template>\n  <section>\n    <h2>{{ question }}</h2>\n    <label for=\"Answers\"> Answer:</label>\n    <input\n      v-model=\"userAnswer\"\n      type=\"text\"\n      autofocus\n      @keydown.enter.prevent=\"checkAnswer\"\n    />\n    <base-dialog\n      v-if=\"showDialog\"\n      title=\"Jumble word lesson\"\n      @close=\"confirmError\"\n    >\n      <template #default>\n        <p>{{ dialogMsg }}</p>\n      </template>\n      <template #actions>\n        <base-button @click=\"confirmError\" v-show=\"!correctAnswer\"\n          >Retry</base-button\n        >\n        <base-button @click=\"goToHomepage\">Go to Homepage</base-button>\n      </template>\n    </base-dialog>\n  </section>\n</template>\n\n<script>\nexport default {\n  inject: ['keyboard'],\n  props: ['lessonId'],\n  data() {\n    return {\n      answer: '',\n      question: '',\n      userAnswer: '',\n      showDialog: false,\n      dialogMsg: '',\n      correctAnswer: false,\n    };\n  },\n  methods: {\n    loadKeyboardQuestion(lessonId) {\n      const selectedLesson = this.keyboard.find(\n        (lesson) => lesson.id == lessonId\n      );\n      if (selectedLesson) {\n        this.question = selectedLesson.question;\n        this.answer = selectedLesson.answer;\n      }\n    },\n    checkAnswer() {\n      if (this.userAnswer.toLowerCase() === this.answer.toLowerCase()) {\n        this.correctAnswer = true;\n        this.dialogMsg = 'That is correct answer!';\n      } else {\n        this.correctAnswer = false;\n        this.dialogMsg = 'That is wrong answer!';\n      }\n      this.showDialog = true;\n    },\n    confirmError() {\n      this.showDialog = false;\n    },\n    goToHomepage() {\n      this.$router.push({ name: 'homepage' });\n    },\n  },\n  created() {\n    this.loadKeyboardQuestion(this.lessonId);\n  },\n  watch: {\n    lessonId(newId) {\n      this.loadKeyboardQuestion(newId);\n    },\n  },\n};\n</script>\n\n<style scoped>\nsection {\n  margin: 2rem auto;\n  max-width: 40rem;\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.26);\n  padding: 1rem;\n  border-radius: 12px;\n}\n\nh2 {\n  margin: 0.5rem 0;\n}\n\nul {\n  list-style: none;\n  margin: 0;\n  padding: 0;\n}\n\nlabel {\n  font-weight: bold;\n  display: block;\n  margin-bottom: 0.5rem;\n}\n\ninput,\ntextarea {\n  display: block;\n  width: 100%;\n  font: inherit;\n  padding: 0.15rem;\n  border: 1px solid #ccc;\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.26);\n}\n\ninput:focus,\ntextarea:focus {\n  outline: none;\n  border-color: #3a0061;\n  background-color: #f7ebff;\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.26);\n}\n\n.form-control {\n  margin: 1rem 0;\n}\n</style>"]}]}